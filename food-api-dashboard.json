{
    "dashboard": {
        "id": null,
        "title": "Food API - Agriculture Monitoring Dashboard",
        "tags": [
            "food-api",
            "agriculture",
            "monitoring"
        ],
        "style": "dark",
        "timezone": "browser",
        "panels": [
            {
                "id": 1,
                "title": "Food API - Orders Created",
                "type": "stat",
                "targets": [
                    {
                        "expr": "foodapi_orders_created_total",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                },
                                {
                                    "color": "yellow",
                                    "value": 30
                                },
                                {
                                    "color": "red",
                                    "value": 50
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 0,
                    "y": 0
                }
            },
            {
                "id": 2,
                "title": "Active Users",
                "type": "stat",
                "targets": [
                    {
                        "expr": "foodapi_active_users",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                },
                                {
                                    "color": "yellow",
                                    "value": 10
                                },
                                {
                                    "color": "red",
                                    "value": 20
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 6,
                    "y": 0
                }
            },
            {
                "id": 3,
                "title": "Crops Monitored",
                "type": "stat",
                "targets": [
                    {
                        "expr": "foodapi_crops_monitored",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                },
                                {
                                    "color": "yellow",
                                    "value": 100
                                },
                                {
                                    "color": "red",
                                    "value": 200
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 12,
                    "y": 0
                }
            },
            {
                "id": 4,
                "title": "Order Processing Duration",
                "type": "stat",
                "targets": [
                    {
                        "expr": "rate(foodapi_order_duration_seconds_sum[5m]) / rate(foodapi_order_duration_seconds_count[5m])",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "s",
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                },
                                {
                                    "color": "yellow",
                                    "value": 0.5
                                },
                                {
                                    "color": "red",
                                    "value": 1
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 18,
                    "y": 0
                }
            },
            {
                "id": 5,
                "title": "Order Processing Time Distribution",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "histogram_quantile(0.50, rate(foodapi_order_duration_seconds_bucket[5m]))",
                        "refId": "A",
                        "legendFormat": "50th percentile"
                    },
                    {
                        "expr": "histogram_quantile(0.95, rate(foodapi_order_duration_seconds_bucket[5m]))",
                        "refId": "B",
                        "legendFormat": "95th percentile"
                    },
                    {
                        "expr": "histogram_quantile(0.99, rate(foodapi_order_duration_seconds_bucket[5m]))",
                        "refId": "C",
                        "legendFormat": "99th percentile"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "s"
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 8
                }
            },
            {
                "id": 6,
                "title": "System Resource Usage",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "rate(container_cpu_usage_seconds_total{pod=~\"foodapi-metrics.*\"}[5m]) * 100",
                        "refId": "A",
                        "legendFormat": "CPU Usage %"
                    },
                    {
                        "expr": "container_memory_working_set_bytes{pod=~\"foodapi-metrics.*\"} / 1024 / 1024",
                        "refId": "B",
                        "legendFormat": "Memory Usage MB"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 8
                }
            }
        ],
        "time": {
            "from": "now-1h",
            "to": "now"
        },
        "refresh": "5s"
    }
}